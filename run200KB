echo "System specs"  >report200KB.txt
echo ""  >>report200KB.txt
echo "Precessor - cat /proc/cpuinfo"  >>report200KB.txt
echo ""  >>report200KB.txt
cat /proc/cpuinfo >>report200KB.txt
echo ""  >>report200KB.txt
echo "Precessor - lscpu"  >>report200KB.txt
echo ""  >>report200KB.txt
lscpu >>report200KB.txt
echo ""  >>report200KB.txt
echo "Memory"  >>report200KB.txt
echo ""  >>report200KB.txt
cat /proc/meminfo >>report200KB.txt
echo ""  >>report200KB.txt

