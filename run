echo "System specs"  >report4KB.txt
echo ""  >>report4KB.txt
echo "Precessor - cat /proc/cpuinfo"  >>report4KB.txt
echo ""  >>report4KB.txt
cat /proc/cpuinfo >>report4KB.txt
echo ""  >>report4KB.txt
